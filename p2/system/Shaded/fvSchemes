FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Gauss harmonic corrected;
}

gradSchemes
{
    default         Gauss harmonic corrected;
}

divSchemes
{
    default         Gauss harmonic corrected;
}

laplacianSchemes
{
    default             Gauss harmonic corrected;
    laplacian(alpha,h)  Gauss harmonic corrected;
}

interpolationSchemes
{
    default          harmonic ;
}

snGradSchemes
{
    default         dGauss harmonic corrected;
}


