FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/Shaded";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    topWall
    {
        type            calculated;
        value           uniform 100000;
    }
    rightWall
    {
        type            calculated;
        value           uniform 100000;
    }
    defaultFaces
    {
        type            empty;
    }
    Shaded_to_unShaded
    {
        type            calculated;
        value           uniform 0;
    }
}


