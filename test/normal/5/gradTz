FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5";
    object      gradTz;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    adiabatic
    {
        type            calculated;
        value           uniform 0;
    }
    tw1
    {
        type            calculated;
        value           uniform 0;
    }
    tw2
    {
        type            calculated;
        value           uniform 0;
    }
    convRad
    {
        type            calculated;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


