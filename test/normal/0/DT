FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      DT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 12;

boundaryField
{
    adiabatic
    {
        type            zeroGradient;
    }
    tw1
    {
        type            zeroGradient;
    }
    tw2
    {
        type            zeroGradient;
    }
    convRad
    {
        type            zeroGradient;
    }
}


